{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "declaration": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "inlineSources": true,
        "outDir": "./lib-es2015",
        "lib": [
            "es2015"
        ]
    },
    "include": [
        "./modules/*.ts"
    ]
}
