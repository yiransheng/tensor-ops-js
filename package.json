{
  "name": "tensor-ops",
  "version": "0.0.2",
  "description": "tensor operations modeled off numpy and tensorflow",
  "main": "lib/index.js",
  "jsnext:main": "lib-es2015/index.js",
  "scripts": {
    "clean": "rimraf lib lib-es2015",
    "test": "tape -r babel-polyfill -r babel-register './test/*.test.js'",
    "prepublish": "npm run clean && npm run build",

    "compileTypescript": "mkdir -p ./lib-es2015 && tsc",
    "tslint": "tslint --exclude 'node_modules/**' 'modules/*.ts'",
    "build": "npm run clean && npm run compileTypescript && babel lib-es2015 --out-dir lib"
  },
  "keywords": [
    "tensor",
    "tensorflow",
    "numpy",
    "matrix"
  ],
  "author": "Yiran Sheng",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yiransheng/tensor-ops-js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.3.13",
    "babel-plugin-transform-es3-property-literals": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.7.2",
    "lodash": "4.17.2",
    "rimraf": "2.4.4",
    "tape": "4.5.1"
  }
}
